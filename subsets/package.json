{
  "name": "subsets",
  "private": true,
  "workspaces": [
    "./apps/*",
    "./packages/*"
  ],
  "scripts": {
    "native": "cd apps/expo && yarn start",
    "watch": "yarn workspaces foreach -pi run watch",
    "vercel:install": "yarn set version 3.5 && yarn install",
    "web": "yarn build && cd apps/next && yarn next",
    "web:extract": "DISABLE_EXTRACTION=false yarn workspace next-app dev",
    "web:prod": "yarn workspace next-app build",
    "web:prod:serve": "yarn workspace next-app serve",
    "fix": "manypkg fix",
    "postinstall": "yarn check-deps && yarn build",
    "build": "yarn workspaces foreach --all --exclude next-app run build",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest react-native-web-lite@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary react-native-web-lite@canary",
    "check-deps": "check-dependency-version-consistency .",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.3.0",
    "react-native-web": "~0.19.12"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.6",
    "@manypkg/cli": "^0.21.4",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-clipboard/clipboard": "^1.14.2",
    "check-dependency-version-consistency": "^4.1.0",
    "expo": "~51.0.28",
    "lottie-react-native": "^7.0.0",
    "node-gyp": "^10.1.0",
    "prettier": "^3.2.5",
    "react-native": "0.74.5",
    "react-native-clipboard": "^0.0.5",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-reanimated": "^3.15.3",
    "turbo": "^1.13.3",
    "typescript": "4.8.2"
  },
  "packageManager": "yarn@3.5.1",
  "devDependencies": {
    "@next/eslint-plugin-next": "12.0.0",
    "@types/eslint": "8",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0"
  },
  "version": "1.0.0"
}
